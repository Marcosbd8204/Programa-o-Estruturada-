//Questão 01
#include <stdio.h>
#include <stdlib.h>

int main(){
    char srt[5]="Porta";
    int i=0;
    for (i=0;i<5;i++){
        printf("%c",srt[i]);
    }
    return 0;
}
//Questão 02
#include <stdio.h>
#include <stdlib.h>

int main(){
    char srt[8]="Manivela";
    int i=0, cont=0;
    for (i=0;i<8;i++){
        printf("%c",srt[i]);
        cont++;
    }
    printf("O tamanho dessa string é de %d caracteres.",cont);
    return 0;
}
//Questão 03
#include <stdio.h>
#include <stdlib.h>

int main(){
    char srt[8]="Manivela",srt2[8]="Manivela";
    int i=0, cont=0;
    for (i=0;i<8;i++){
        if( srt[i] == srt2[i]){
        }else{
            cont++;
        }
    }
    if (cont==0){
        printf("as palavras são iguais");
    }else{
        printf("as palavras são diferentes");
    }
    return 0;
}
//Questão 04
#include <stdio.h>
#include <stdlib.h>

int main(){
    char srt[6]="panela";
    int i=0;
    for (i=0;i<6;i++){
        if(i<4){
            printf("%c",srt[i]);
        }else{
        }
    }
    return 0;
}
//Questão 05
#include <stdio.h>
#include <stdlib.h>

int main(){
    char srt[7]="Bananas";
    int i=0,cont=0;
    for (i=0;i<7;i++){
       cont++;
    }
    printf("Essa Palavra tem %d número de letras",cont);
    return 0;
}
//Questão 06
#include <stdio.h>
#include <stdlib.h>

int main(){
    char nome[6]="maria", sexo[1]="f";
    int i=0, idade=25;
    
    if(sexo[1] == "f" && idade<=25){
        printf("Você foi aceita");
    }else{
        printf("Você não foi aceita");
    }
    return 0;
}
//Questão 07 incompleta
#include <stdio.h>
#include <stdlib.h>

int main() {
    char string[20]="0111101000", l=1;
    int i=0,cont=0;
    for(i=0;i<20;i++){
      if(string[i]==l){
          cont++;
      }else{
      }
    }
    printf("o texto inteiro é:  %s \n e o numero de 1 é: %d ",string,cont);
    return 0;
//Questão 08 incompleto
